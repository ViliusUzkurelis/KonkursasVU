            <a href="/miestai">Miestai</a>
            <a href="/miestai/pasirinkti">Pasirinkti miestai</a>
        </nav>
    </header>
    <main>
        <article>
            <div>
                <h1>Miestai</h1>
                <h1>Pasirinkite savivaldybe</h1>
                <div class="cities">
                <% ten.forEach(city => { %>
                    <div>
                        <a href="/miestai/<%= city %>"><%= city %></a>
                    </div>
                <% }) %>
            </div>

            </div>
        </article>
    </main>
    <footer>Poraštė</footer>
</body>

</html>